{
  "name": "decentralized-renderer-react-template",
  "version": "0.0.0-development",
  "private": true,
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "dependencies": {
    "@ahryman40k/ts-fhir-types": "^4.0.36",
    "@emotion/babel-plugin": "^11.10.5",
    "@emotion/css": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@govtechsg/decentralized-renderer-react-components": "^3.5.2",
    "@govtechsg/oa-schemata": "^1.18.2",
    "@hot-loader/react-dom": "^17.0.1",
    "@notarise-gov-sg/i18n-iso-countries": "^7.3.1",
    "crypto-browserify": "^3.12.0",
    "debug": "^4.3.2",
    "eslint-mdx": "^2.0.5",
    "fhir": "^4.10.0",
    "qrcode": "^1.5.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hot-loader": "^4.12.18",
    "stream-browserify": "^3.0.0"
  },
  "engines": {
    "npm": ">=9"
  },
  "scripts": {
    "build": "npm run check-types && cross-env NODE_ENV=production webpack --progress --mode production",
    "deploy": "rm -rf out && npm run build",
    "check-types": "tsc --sourceMap false --noEmit",
    "dev": "cross-env NODE_ENV=development webpack-dev-server",
    "example:application": "webpack-dev-server --config application/webpack.config.js",
    "lint": "eslint . --ext .js,.ts,.tsx,.mdx --max-warnings 0",
    "lint:fix": "npm run lint -- --fix",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook -o docs",
    "test": "jest --ci",
    "test:coverage": "npm run test -- --coverage",
    "test:watch": "npm run test -- --watch",
    "start": "webpack-dev-server --hot --mode development"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.14.5",
    "@babel/plugin-proposal-optional-chaining": "^7.14.5",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@storybook/addon-actions": "^6.5.14",
    "@storybook/addon-docs": "^7.6.17",
    "@storybook/addon-knobs": "^6.4.0",
    "@storybook/addon-viewport": "^6.5.14",
    "@storybook/addons": "^6.5.14",
    "@storybook/builder-webpack5": "^6.5.16",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/react": "^7.6.17",
    "@testing-library/react": "^9.3.2",
    "@types/debug": "^4.1.7",
    "@types/jest": "^27.5.2",
    "@types/lodash.clonedeep": "^4.5.6",
    "@types/mdx": "^2.0.3",
    "@types/node": "^18.11.12",
    "@types/qrcode": "^1.4.2",
    "@types/react": "^18.0.26",
    "@typescript-eslint/eslint-plugin": "^5.45.1",
    "@typescript-eslint/parser": "^5.45.1",
    "@webpack-cli/serve": "^1.7.0",
    "babel-jest": "^27.5.1",
    "babel-loader": "^8.2.2",
    "brotli-webpack-plugin": "1.1.0",
    "compression-webpack-plugin": "^10.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^27.1.6",
    "eslint-plugin-mdx": "^2.0.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.25.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "4.2.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^27.5.1",
    "jest-canvas-mock": "^2.3.1",
    "lodash.clonedeep": "^4.5.0",
    "node-polyfill-webpack-plugin": "^2.0.1",
    "prettier": "^2.8.1",
    "raw-loader": "^4.0.2",
    "typescript": "^4.4.2",
    "webpack": "^5.90.3",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.0"
  },
  "license": "Apache-2.0",
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 Opera versions",
    "last 2 Firefox versions",
    "last 2 Edge versions",
    "last 1 Safari version",
    "IE >= 11"
  ],
  "jest": {
    "testEnvironment": "jsdom",
    "coverageDirectory": "coverage",
    "testRegex": "(/__tests__/.*|(\\.|/)(test))\\.tsx?$",
    "moduleNameMapper": {
      "\\.(jpg|ico|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/fileMock.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Open-Attestation/decentralized-renderer-react-template.git"
  }
}